<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
      
        <record id="seq_incentive_report" model="ir.sequence">
            <field name="name">Incentive Report</field>
            <field name="code">sale.incentive</field>
            <field name="prefix">INC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_employee_code" model="ir.sequence">
			<field name="name">Employee</field>
			<field name="code">hr.employee.code</field>
			<field name="prefix">EMP/</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

    </data>
</odoo>